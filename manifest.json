{
    "name":"E-Downloader",
    "version":"1.0",
    "manifest_version":2,
    "description":"A all in one Downloader",
    "content_scripts":[{
        "matches":["<all_urls>"],
        "js":["content.js"]
    }],
    "browser_action":{
        "default_popup":"popup.html",
        "default_name":"E-Downloader"
    },
    "icons": {"16": "icon.png"}
}